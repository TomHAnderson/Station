{
    "name": "sfphp/sfphp.org",
    "description": "sfphp.org",
    "type": "library",
    "license": "GPL-2.0",
    "keywords": [
        "sfphp",
        "zf2"
    ],
    "homepage": "http://sfphp.org/",
    "support": {
        "email": "php-139@meetup.com",
        "source": "https://github.com/sfphp/sfphp.org",
        "issues": "https://github.com/sfphp/sfphp.org/issues"
    },
    "authors": [
        {
            "name": "Mike Tougeron",
            "email": "michael@sfphp.org"
        },
        {
            "name": "Jacob Mather",
            "email": "jacob@sfphp.org"
        },
        {
            "name": "Tom Anderson",
            "email": "tom.h.anderson@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "process-timeout": 5000
    },
    "require": {
        "php": ">=5.5.0",
        "zendframework/zendframework": ">=2.3.2,<3.0.0",
        "guzzlehttp/guzzle": "5.0.*",
        "zfcampus/zf-apigility": "~1.0",
        "zfcampus/zf-apigility-documentation": "~1.0",
        "zfcampus/zf-development-mode": "~2.0",
        "zfcampus/zf-apigility-doctrine": "0.2.*",
        "hounddog/doctrine-data-fixture-module": "0.0.*",
        "dms/meetup-api-client": "1.0.x-dev",
        "zf-commons/zfc-user-doctrine-orm": "1.*",
        "socalnick/scn-social-auth-doctrine-orm": "1.4.*",
        "fabpot/php-cs-fixer": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.3.*",
        "mockery/mockery": ">=0.7.2",
        "zendframework/zftool": "dev-master",
        "zendframework/zend-developer-tools": "dev-master",
        "zfcampus/zf-apigility-admin": "~1.0",
        "zfcampus/zf-apigility-welcome": "~1.0",
        "zfcampus/zf-deploy": "~1.0",
        "fzaninotto/faker": "dev-master"
    },
    "autoload": {
        "classmap": ["module/", "vendor/"]
    }
}
