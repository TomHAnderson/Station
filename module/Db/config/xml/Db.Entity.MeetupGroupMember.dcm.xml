<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xsi="http://www.w3.org/2001/XMLSchema-instance" schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Db\Entity\MeetupGroupMember">
    <id name="id" type="integer">
      <generator strategy="AUTO"/>
    </id>
    <field name="role" type="string" nullable="true"/>
    <many-to-one field="meetupGroup" target-entity="Db\Entity\MeetupGroup" inversed-by="meetupGroupMember">
      <join-columns>
        <join-column name="meetup_group_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="member" target-entity="Db\Entity\Member" inversed-by="meetupGroupMember">
      <join-columns>
        <join-column name="member_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
